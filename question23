mark=int(input("enterthe mark"))
if mark<25:
    print("grade f")
elif mark>=25 and mark<=45:
    print("grade e")
elif mark>=45 and mark<=50:
    print("grade d")
elif mark>=50 and mark<=60:
    print("grade c")
elif mark>=60 and mark<=80:
    print("grade b")
elif mark>80:
    print("grade a")
else:
    print("no grade")